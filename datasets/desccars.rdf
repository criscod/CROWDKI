<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:foaf="http://xmlns.com/foaf/0.1/"
	xmlns:dct="http://purl.org/dc/terms/"
	xmlns:dcat="http://www.w3.org/ns/dcat#"
	xmlns:void="http://rdfs.org/ns/void#"
	xmlns:onto="http://ontoexample.org/onto1#"
	xmlns:crowdki="http://crowdki.uni-koblenz.de/datasets#"> 
  <rdf:Description rdf:about="http://crowdki.uni-koblenz.de/datasets#Dataset1">
   <dct:description>Dataset containing information about cars. </dct:description>
   <dct:language>EN</dct:language>
   <dcat:keyword>vehicles</dcat:keyword>
   <foaf:page rdf:resource="http://datasets.org/d1"/>
   <dct:subject>cars</dct:subject>
   <void:exampleResource rdf:resource="http://example.org/cars#car1"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/cars#car1">
  <rdf:type rdf:resource="http://ontoexample.org/onto1#Car"/>
  <onto:model>Cabio 180</onto:model>
  <onto:price>10,000</onto:price>
  </rdf:Description>
</rdf:RDF>